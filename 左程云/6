int peek_seek(vector<int>nums){
    int n = nums.size();
    int l = 1;
    int r = n-1;
    int mid;
    while (l <= r){
        mid = (l+r) / 2;
        if (nums[mid] > nums[mid+1] && nums[mid] > nums[mid-1]){
            return mid;
        }else{
            if (nums[mid-1] > nums[mid]){
                r = mid-1;
            }else if (nums[mid+1] > nums[mid]){
                l = mid+1;
            } 
        }
    }
    return mid; 
}
